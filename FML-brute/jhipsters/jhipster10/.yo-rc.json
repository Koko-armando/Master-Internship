{
  "generator-jhipster": {
    "baseName": "jhipster",
    "packageName": "io.variability.jhipster",
    "packageFolder": "io/variability/jhipster",
    "authenticationType": "uaa",
    "hibernateCache": "hazelcast",
    "clusteredHttpSession": "false",
    "websocket": "false",
    "databaseType": "sql",
    "devDatabaseType": "h2Memory",
    "prodDatabaseType": "mariadb",
    "searchEngine": "searchEngine:elasticsearch",
    "buildTool": "maven",
    "applicationType": "monolith",
    "testFrameworks": [
      "cucumber",
      "gatling",
      "protractor"
    ],
    "jhipsterVersion": "4.8.2",
    "clientFramework": "angularX",
    "serviceDiscoveryType": "eureka",
    "installModules": false,
    "useSass": false,
    "messageBroker": "messageBroker:kafka",
    "enableTranslation": true,
    "nativeLanguage": "en",
    "languages": [
      "en",
      "fr"
    ],
    "skipClient": false,
    "skipServer": false,
    "enableSocialSignIn": "no",
    "enableSwaggerCodegen": "enableSwaggerCodegen:true"
  }
}