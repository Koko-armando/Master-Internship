{
  "generator-jhipster": {
    "baseName": "jhipster",
    "packageName": "io.variability.jhipster",
    "packageFolder": "io/variability/jhipster",
    "serverPort": "8080",
    "authenticationType": "uaa",
    "hibernateCache": "hazelcast",
    "clusteredHttpSession": "false",
    "websocket": "websocket:spring-websocket",
    "databaseType": "sql",
    "devDatabaseType": "mysql",
    "prodDatabaseType": "mysql",
    "searchEngine": "false",
    "buildTool": "maven",
    "applicationType": "gateway",
    "testFrameworks": [
      "cucumber",
      "protractor",
      "gatling"
    ],
    "jhipsterVersion": "4.8.2",
    "clientFramework": "angular1",
    "serviceDiscoveryType": false,
    "installModules": false,
    "useSass": false,
    "messageBroker": "messageBroker:kafka",
    "enableTranslation": true,
    "nativeLanguage": "en",
    "languages": [
      "en",
      "fr"
    ],
    "skipClient": false,
    "skipServer": false,
    "skipUserManagement": true,
    "enableSocialSignIn": "no",
    "enableSwaggerCodegen": "enableSwaggerCodegen:true",
    "uaaBaseName": "uaa"
  }
}