{
  "generator-jhipster": {
    "baseName": "jhipster",
    "packageName": "io.variability.jhipster",
    "packageFolder": "io/variability/jhipster",
    "serverPort": "8080",
    "authenticationType": "uaa",
    "hibernateCache": "no",
    "clusteredHttpSession": "clusteredHttpSession:hazelcast",
    "websocket": "websocket:spring-websocket",
    "databaseType": "mongodb",
    "devDatabaseType": "mongodb",
    "prodDatabaseType": "mongodb",
    "searchEngine": "no",
    "buildTool": "maven",
    "applicationType": "uaa",
    "testFrameworks": [
      "gatling",
      "cucumber",
      "protractor"
    ],
    "jhipsterVersion": "4.8.2",
    "servicediscoveryType": "false",
    "installModules": false,
    "messageBroker": "messageBroker:kafka",
    "enableTranslation": true,
    "nativeLanguage": "en",
    "languages": [
      "en",
      "fr"
    ],
    "skipClient": true,
    "skipServer": false,
    "enableSocialSignIn": "enableSocialSignIn :true",
    "enableSwaggerCodegen": "enableSwaggerCodegen:true"
  }
}