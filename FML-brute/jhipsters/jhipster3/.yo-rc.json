{
  "generator-jhipster": {
    "baseName": "jhipster",
    "packageName": "io.variability.jhipster",
    "packageFolder": "io/variability/jhipster",
    "serverPort": "8080",
    "authenticationType": "uaa",
    "hibernateCache": "hazelcast",
    "clusteredHttpSession": "clusteredHttpSession:hazelcast",
    "websocket": "false",
    "databaseType": "sql",
    "devDatabaseType": "h2Disk",
    "prodDatabaseType": "mariadb",
    "searchEngine": "searchEngine:elasticsearch",
    "buildTool": "maven",
    "applicationType": "gateway",
    "testFrameworks": [
      "gatling",
      "protractor",
      "cucumber"
    ],
    "jhipsterVersion": "4.8.2",
    "clientFramework": "angular1",
    "serviceDiscoveryType": "consul",
    "installModules": false,
    "useSass": false,
    "messageBroker": "false",
    "enableTranslation": true,
    "nativeLanguage": "en",
    "languages": [
      "en",
      "fr"
    ],
    "skipClient": false,
    "skipServer": false,
    "skipUserManagement": true,
    "enableSocialSignIn": "no",
    "enableSwaggerCodegen": "no",
    "uaaBaseName": "uaa",
    "clientPackageManager": "yarn",
    "jhiPrefix": "jhi"
  }
}